
{
    "manifest_version": 3,
    "name": "Hello World Alert",
    "version": "1.1",
    "description": "An extension that alerts 'Hello world' on the current page.",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "storage"
    ],
    "host_permissions": [
    "https://open.kattis.com/*",
    "https://api.github.com/*"
    ]
}
  
